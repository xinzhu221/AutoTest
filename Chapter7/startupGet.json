[
  {
    "description":"模拟一个没有参数的get请求",
    "request":{
      "uri":"/getdemo",
      "method":"get"
    },
    "response":{
      "headers":{
        "Content-Type":"text/html;charset=gbk"
      },
      "text":"这是一个没有参数的get请求"
    }
  },
  {
    "description":"有参数的get请求",
    "request":{
      "uri":"/getwithparam",
      "method":"get",
      "queries":{
        "name":"kk",
        "age":"18"
      }
    },
    "response":{
      "headers":{
        "Content-Type":"text/html;charset=gbk"
      },
      "text":"kk回来啦"
    }
  }
]